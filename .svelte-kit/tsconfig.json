{
	"compilerOptions": {
		"paths": {
			"$lib": [
				"../src/lib"
			],
			"$lib/*": [
				"../src/lib/*"
			],
			"$styles": [
				"../src/styles"
			],
			"$styles/*": [
				"../src/styles/*"
			],
			"@/*": [
				"../src/lib/*"
			],
			"$assets": [
				"../src/lib/assets"
			],
			"$assets/*": [
				"../src/lib/assets/*"
			],
			"$components": [
				"../src/lib/components"
			],
			"$components/*": [
				"../src/lib/components/*"
			],
			"$ui": [
				"../src/lib/components/ui"
			],
			"$ui/*": [
				"../src/lib/components/ui/*"
			],
			"$features": [
				"../src/lib/components/features"
			],
			"$features/*": [
				"../src/lib/components/features/*"
			],
			"$hooks": [
				"../src/lib/hooks"
			],
			"$hooks/*": [
				"../src/lib/hooks/*"
			],
			"$utils": [
				"../src/lib/utils"
			],
			"$utils/*": [
				"../src/lib/utils/*"
			],
			"$vendor": [
				"../src/lib/vendor"
			],
			"$vendor/*": [
				"../src/lib/vendor/*"
			],
			"$vendorShadcn": [
				"../src/lib/vendor/shadcn"
			],
			"$vendorShadcn/*": [
				"../src/lib/vendor/shadcn/*"
			],
			"$vendorShadcnExtra": [
				"../src/lib/vendor/shadcn-extra"
			],
			"$vendorShadcnExtra/*": [
				"../src/lib/vendor/shadcn-extra/*"
			],
			"$app/types": [
				"./types/index.d.ts"
			]
		},
		"rootDirs": [
			"..",
			"./types"
		],
		"verbatimModuleSyntax": true,
		"isolatedModules": true,
		"lib": [
			"esnext",
			"DOM",
			"DOM.Iterable"
		],
		"moduleResolution": "bundler",
		"module": "esnext",
		"noEmit": true,
		"target": "esnext"
	},
	"include": [
		"ambient.d.ts",
		"non-ambient.d.ts",
		"./types/**/$types.d.ts",
		"../vite.config.js",
		"../vite.config.ts",
		"../src/**/*.js",
		"../src/**/*.ts",
		"../src/**/*.svelte",
		"../tests/**/*.js",
		"../tests/**/*.ts",
		"../tests/**/*.svelte"
	],
	"exclude": [
		"../node_modules/**",
		"../src/service-worker.js",
		"../src/service-worker/**/*.js",
		"../src/service-worker.ts",
		"../src/service-worker/**/*.ts",
		"../src/service-worker.d.ts",
		"../src/service-worker/**/*.d.ts"
	]
}